<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <GeneDef ParentName="GeneSleepBase">
        <defName>ExtremelySleepy</defName>
        <label>extremely sleepy</label>
        <description>Carriers of this gene get tired extremely fast compared to others. Note: This gene results in a pawn needing to make full rests more than twice a day.</description>
        <iconPath>UI/Icons/Genes/Gene_VerySleepy</iconPath>
        <displayOrderInCategory>-2</displayOrderInCategory>
        <statFactors>
            <RestFallRateFactor>2.2</RestFallRateFactor>
        </statFactors>
        <biostatMet>6</biostatMet>
        <biostatCpx>3</biostatCpx>
        <symbolPack>
            <prefixSymbols>
                <li><symbol>sleep</symbol></li>
                <li><symbol>bed</symbol></li>
                <li><symbol>somno</symbol></li>
            </prefixSymbols>
            <suffixSymbols>
                <li><symbol>loaf</symbol></li>
            </suffixSymbols>
        </symbolPack>
    </GeneDef>

